{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "@tabler/core@1.1.1",
      "destination": "wwwroot/libs/tabler",
      "files":[
        "dist/css/tabler.min.css",
        "dist/css/tabler.min.css.map",
        "dist/css/tabler-vendors.min.css",
        "dist/css/tabler-vendors.min.css.map",
        "dist/js/tabler.min.js",
        "dist/js/tabler.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@tabler/icons-webfont@latest",
      "destination": "wwwroot/libs/tabler-icons",
      "files": [
        "dist/tabler-icons.min.css",
        "dist/tabler-icons.min.css.map",
        "dist/fonts/*"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "tom-select@latest",
      "destination": "wwwroot/libs/tom-select",
      "files":[
        "dist/js/tom-select.base.min.js",
        "dist/js/tom-select.base.min.js.map",
        "dist/css/tom-select.bootstrap5.css",
        "dist/css/tom-select.bootstrap5.css.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "two.js@latest",
      "destination": "wwwroot/libs/two.js",
      "files": [
        "build/two.min.js",
        "extras/js/zui.js"
      ]
    }
  ]
}