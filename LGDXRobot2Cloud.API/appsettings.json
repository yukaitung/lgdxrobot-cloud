{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "https://*:5162",
        "Protocols": "Http2",
        "Certificate": {
          "Path": "grpc.pfx"
        }
      },
      "Https": {
        "Url": "https://*:5163",
        "Protocols": "Http1AndHttp2"
      }
    }
  }
}
