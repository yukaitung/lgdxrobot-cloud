{
  "ConnectionStrings": {
    "Default": "Host=postgres;Username=admin;Password=mysecretpassword;Database=LGDXRobotCloud;",
    "Activity": "Host=postgres;Username=admin;Password=mysecretpassword;Database=LGDXRobotCloudLogs;"
  },
  "RabbitMq": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
